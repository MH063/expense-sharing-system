    "INSERT INTO system_performance_metrics (metric_name, metric_value, metric_unit, additional_data)\nVALUES\n('cpu_usage', 25.5, '%', '{\"cores\": 4}'),\n('memory_usage', 60.2, '%', '{\"total\": \"8GB\", \"used\": \"4.8GB\"}'),\n('disk_usage', 45.8, '%', '{\"total\": \"100GB\", \"used\": \"45.8GB\"}');",
